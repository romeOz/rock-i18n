{
    "name": "romeoz/rock-i18n",
    "description": "i18n library for PHP",
    "keywords": ["i18n", "internationalization", "localized", "l10n"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-i18n/issues",
        "source": "https://github.com/romeOz/rock-i18n"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "0.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.0"
    },
    "suggest": {
        "romeoz/rock-di": "required by module Rock Dependency Injection"
    },
    "autoload": {
        "psr-4": {
            "rock\\i18n\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}